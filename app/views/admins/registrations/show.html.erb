    <p>
        <strong>Nombre</strong>
        <%= @admin.nombre %>
    </p>

    <p>
        <strong>Email</strong>
        <%= @admin.email %>
    </p>
    <br>
    <%= link_to 'Editar Admin', admin_edit_admin_path(@admin.id) %><br>
    <br>
    <%= link_to "Volver a lista de Admins", lista_admins_path %>
<% else %>
    <% controller.redirect_to new_admin_session_path, notice: 'No tienes permiso para ingresar a la pÃ¡gina que intentaste acceder.' %>
<% end %>